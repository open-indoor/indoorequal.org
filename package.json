{
  "name": "openindoor.io",
  "version": "0.0.1",
  "author": "OpenIndoor",
  "license": "BSD-3-Clause",
  "scripts": {
    "dev": "parcel serve parcel --hmr-port 1235 index.html",
    "build": "parcel build index.html",
    "test": "jest --watch",
    "extract-poi-mapping": "node ./script/extract-poi-mapping"
  },
  "browserslist": "last 2 Firefox versions, last 2 Chrome versions, last 2 Edge versions, last 2 Safari versions",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "vue-jest"
    },
    "moduleNameMapper": {
      "\\.svg$": "<rootDir>/test/file_mock.js"
    }
  },
  "htmlnano": {
    "minifySvg": false
  },
  "posthtml": {
    "recognizeSelfClosing": true
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@mdi/font": "^5.6.55",
    "@vue/component-compiler-utils": "^3.2.0",
    "@vue/test-utils": "^1.1.0",
    "babel-jest": "^26.3.0",
    "deepmerge": "^4.2.2",
    "jest-cli": "^26.4.2",
    "js-yaml": "^3.14.0",
    "minipass-fetch": "^1.2.1",
    "parcel": "",
    "parcel-plugin-static-files-copy": "",
    "vue-hot-reload-api": "^2.3.3",
    "vue-jest": "^4.0.0-beta.5",
    "vue-template-compiler": "^2.6.9"
  },
  "dependencies": {
    "country-language": "^0.1.7",
    "i18next-client": "^1.11.1",
    "luxon": "^1.22.0",
    "mapbox-gl": "^1.12.0",
    "mapbox-gl-openindoor": "0.2.7-openindoor",
    "moment": "^2.28.0",
    "opening_hours": "^3.5.0",
    "vue": "^2.6.9",
    "vue-i18n": "^8.21.1",
    "vue-mapbox": "osmontrouge/vue-mapbox#update",
    "vuetify": "^2.2.3"
  }
}
